package interficies;

interface FiguraGeometrica {

    abstract double calculArea()
    abstract double calculPerimetre()
    

public abstract class Figura2D implements FiguraGeometrica(){

    double longitud , ancho;

     cuadrilatero_rectangulo(double 1 , double a){

        this.longitud = 1;
        this.ancho = a;

     }  

     @Override
     public double calculPerimetre(){
        retun ((2*longitud)+(2*ancho));
     }


}



class Main {
    public static final void main (String[] args) {
        Cercle resultat_area = new Cercle();
        Cercle resultat_perimetre = new Cercle();
        Rectangle resultat_area = new Rectangle();
        Rectangle resultat_perimetre = new Rectangle();
        System.out.println(resultat_area.calculArea());
        System.out.println(resultat_perimetre.calculPerimetre());
    }
}

}